@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-2/date-day-precision.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # The calendar date 1985 April 12th with day precision.
  edtfo:inDateTime "1985-04-12" ;
  time:inDateTime [
    a time:DateTimeDescription ;
    time:unitType time:unitDay ;
    time:day 12 ;
    time:month 4 ;
    time:year 1985
    ]
  .
