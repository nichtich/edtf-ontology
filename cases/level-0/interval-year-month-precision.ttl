@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-0/interval-year-month-precision.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:ProperInterval ;
  # A time interval beginning sometime in 2005
  # and ending sometime in February 2006.
  # Since the start endpoint precision (year) is different than that of
  # the end endpoint (month) the precision of the time interval at
  # large is undefined.
  edtfo:hasEDTFDateTimeDescription "2005/2006-02" ;
  time:hasBeginning [
    a time:Instant ;
    time:inDateTime [
      a time:DateTimeDescription ;
      time:unitType time:unitYear ;
      time:year 2005
      ]
    ]
  ;
  time:hasEnd [
    a time:Instant ;
    time:inDateTime [
      a time:DateTimeDescription ;
      time:unitType time:unitMonth ;
      time:year 2006 ;
      time:month 2
      ]
    ]
  .
