@prefix : <https://periodo.github.io/edtf-ontology/cases/level-0/interval/day-year-precision/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
:when a time:Interval .
:when time:hasBeginning _:b1 .
:when time:hasEnd _:b0 .
_:b0 a time:Instant .
_:b0 time:inDateTime _:b3 .
_:b1 a time:Instant .
_:b1 time:inDateTime _:b2 .
_:b2 a time:DateTimeDescription .
_:b2 time:day "---01"^^xsd:gDay .
_:b2 time:month "--02"^^xsd:gMonth .
_:b2 time:unitType time:unitDay .
_:b2 time:year "2004"^^xsd:gYear .
_:b3 a time:DateTimeDescription .
_:b3 time:unitType time:unitYear .
_:b3 time:year "2005"^^xsd:gYear .
