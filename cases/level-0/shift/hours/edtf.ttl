@base <https://periodo.github.io/edtf-ontology/> .
@prefix : <cases/level-0/shift/hours/> .
@prefix edtfo: <edtfo.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:when1 edtfo:inEDTFDateTime "1985-04-12T23:20:30+04" ;
  rdfs:label "April 12th, 1985 at 23:20:30 with a time shift of 4 hours ahead of UTC" .

:when2 edtfo:inEDTFDateTime "1985-04-12T23:20:30-04" ;
  rdfs:label "April 12th, 1985 at 23:20:30 with a time shift of 4 hours behind UTC" .
