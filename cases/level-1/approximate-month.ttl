@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-1/approximate-month.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # Year-month approximate.
  edtfo:inEDTFDateTime "2004-06~" ;
  time:inDateTime :2004_06
  .

:2004_06 a time:DateTimeDescription ;
  time:unitType time:unitMonth ;
  time:year 2004
  time:month 6
  .

:temporalPositionStatement a edtfo:ApproximateStatement ;
  rdf:subject :when ;
  rdf:predicate time:inDateTime ;
  rdf:object :2004_06
  .
