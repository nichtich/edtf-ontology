@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-1/day-unspecified.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # Year and month specified, day unspecified
  # in a year-month-day expression (day precision).
  edtfo:inEDTFDateTime "1985-04-XX" ;
  time:inDateTime  [
    a time:DateTimeDescription ;
    time:unitType time:unitDay ;
    time:year 1985
    time:month 4
    ]
  .
