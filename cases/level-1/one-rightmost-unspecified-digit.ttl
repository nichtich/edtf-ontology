@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-1/one-rightmost-unspecified-digit.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # A year with one rightmost unspecified digit
  # in a year-only expression (year precision).
  edtfo:inEDTFDateTime "201X" ;
  time:inDateTime  [
    a edtfo:DecadeDescription ;
    time:unitType time:unitYear ;
    edtfo:decade 201
    ]
  .
