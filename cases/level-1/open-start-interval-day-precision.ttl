@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-1/open-start-interval-day-precision.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a edtfo:OpenBeginningInterval ;
  # Interval with open start; ending 1985 April 12th with day precision.
  # Double-dot (“..”) may be used when either the start or end date is
  # not specified, either because there is none or for any other
  # reason.
  edtfo:hasEDTFDateTimeDescription "../1985-04-12" ;
  time:hasEnd [
    a time:Instant ;
    time:inDateTime [
      a time:DateTimeDescription ;
      time:unitType time:unitDay ;
      time:year 1985 ;
      time:month 4 ;
      time:day 12
      ]
    ]
  .
