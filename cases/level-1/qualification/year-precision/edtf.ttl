@base <https://periodo.github.io/edtf-ontology/> .
@prefix : <cases/level-1/qualification/year-precision/> .
@prefix edtfo: <edtfo.ttl#> .

# Year uncertain
# (possibly the year 1984, but not definitely).
:when1 edtfo:inEDTFDateTime "1984?" .

# Year approximate
# (approximately 1984).
:when2 edtfo:inEDTFDateTime "1984~" .

# Year uncertain and approximate
# (possibly approximately 1984, but not definitely).
:when3 edtfo:inEDTFDateTime "1984%" .
