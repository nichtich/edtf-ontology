@base <https://periodo.github.io/edtf-ontology/> .

@prefix : <cases/level-1/two-rightmost-unspecified-digits.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # A year with two rightmost unspecified digits
  # in a year-only expression (year precision).
  edtfo:inEDTFDateTime "20XX" ;
  time:inDateTime  [
    a edtfo:CenturyDescription ;
    time:unitType time:unitYear ;
    edtfo:century 20
    ]
  .
