@base <https://periodo.github.io/edtf-ontology/> .

@prefix : <cases/level-1/uncertain-approximate-end-interval-day-precision.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:ProperInterval ;
  # A time interval with calendar day precision,
  # beginning sometime on February 1, 2004
  # and ending sometime on February 8, 2005 (uncertain and approximate).
  edtfo:hasEDTFDateTimeDescription "2004-02-01/2005-02-08%" ;
  time:hasBeginning [
    a time:Instant ;
    time:inDateTime [
      a time:DateTimeDescription ;
      time:unitType time:unitDay ;
      time:year 2004 ;
      time:month 2 ;
      time:day 1
      ]
    ]
  ;
  time:hasEnd :2005_02_08
  .

:2005_02_08 a time:DateTimeDescription ;
  time:unitType time:unitDay ;
  time:year 2005 ;
  time:month 2 ;
  time:day 8
  .

:temporalPositionStatement
  a edtfo:UncertainStatement ;
  a edtfo:ApproximateStatement ;
  rdf:subject :when ;
  rdf:predicate time:hasEnd ;
  rdf:object :2005_02_08
  .
