@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-1/uncertain-start-approximate-end-interval-day-precision.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:ProperInterval ;
  # A time interval with calendar day precision,
  # beginning sometime on February 1, 2004 (uncertain)
  # and ending sometime on February 8, 2005 (approximate).
  edtfo:hasEDTFDateTimeDescription "2004-02-01?/2005-02-08~"
  time:hasBeginning :2004_02_01 ;
  time:hasEnd :2005_02_08
  .

:2004_02_01 a time:DateTimeDescription ;
  time:unitType time:unitDay ;
  time:year 2004 ;
  time:month 2 ;
  time:day 1
  .

:2005_02_08 a time:DateTimeDescription ;
  time:unitType time:unitDay ;
  time:year 2005 ;
  time:month 2 ;
  time:day 8
  .

:temporalPositionStatement
  a edtfo:UncertainStatement ;
  rdf:subject :when ;
  rdf:predicate time:hasBeginning ;
  rdf:object :2004_02_01
  .

:temporalPositionStatement
  a edtfo:ApproximateStatement ;
  rdf:subject :when ;
  rdf:predicate time:hasEnd ;
  rdf:object :2005_02_08
  .
