@prefix edtf: <http://id.loc.gov/datatypes/edtf/> .
@prefix lode: <http://linkedevents.org/ontology/> .

_:something lode:atTime "2004-02-01?/2005-02-08~"^^edtf:EDTF-level1 .
@prefix edtfo: <https://periodo.github.io/edtf-literals/draft/> .
@prefix lode: <http://linkedevents.org/ontology/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

_:something lode:atTime [
    a time:ProperInterval ;
    time:hasBeginning [
      a time:Instant ;
      edtfo:uncertainTemporalPosition [
        a time:DateTimeDescription ;
        time:unitType time:unitDay ;
        time:day "---01"^^xsd:gDay ;
        time:month "--02"^^xsd:gMonth ;
        time:year "2004"^^xsd:gYear
        ]
      ]
    ;
    time:hasEnd [
      a time:Instant ;
      edtfo:approximateTemporalPosition [
        a time:DateTimeDescription ;
        time:unitType time:unitDay ;
        time:day "---08"^^xsd:gDay ;
        time:month "--02"^^xsd:gMonth ;
        time:year "2005"^^xsd:gYear
        ]
      ]
    ]
  .
