@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-1/uncertain-year.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # Year uncertain (possibly the year 1984, but not definitely).
  edtfo:inEDTFDateTime "1984?" ;
  time:inDateTime :1984
  .

:1984 a time:DateTimeDescription ;
  time:unitType time:unitYear ;
  time:year 1984
  .

:temporalPositionStatement a edtfo:UncertainStatement ;
  rdf:subject :when ;
  rdf:predicate time:inDateTime ;
  rdf:object :1984
  .
