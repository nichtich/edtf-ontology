@base <https://periodo.github.io/edtf-ontology/> .

@prefix : <cases/level-2/all-of-set-1.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:When a owl:Class ;
  # All of the years 1667, 1668, 1670, 1671, 1672.
  # Elements immediately preceeding and/or following as well as the elements
  # represented by a double-dot, all have the same precision.
  edtfo:hasEDTFDateTimeSetDescription "{1667,1668,1670..1672}" ;
  owl:disjointUnionOf (
    :TimeIn1667
    :TimeIn1668
    :TimeIn1670
    :TimeIn1671
    :TimeIn1672
    )
  .

:TimeIn1667 a owl:Class ;
  rdfs:subClassOf time:Instant ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty time:inDateTime ;
    owl:someValuesFrom :1667
    ]
  .

:TimeIn1668 a owl:Class ;
  rdfs:subClassOf time:Instant ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty time:inDateTime ;
    owl:someValuesFrom :1668
    ]
  .

:TimeIn1670 a owl:Class ;
  rdfs:subClassOf time:Instant ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty time:inDateTime ;
    owl:someValuesFrom :1670
    ]
  .

:TimeIn1671 a owl:Class ;
  rdfs:subClassOf time:Instant ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty time:inDateTime ;
    owl:someValuesFrom :1671
    ]
  .

:TimeIn1672 a owl:Class ;
  rdfs:subClassOf time:Instant ;
  owl:equivalentClass [
    a owl:Restriction ;
    owl:onProperty time:inDateTime ;
    owl:someValuesFrom :1672
    ]
  .

:YearPrecisionDescription a owl:Class ;
  owl:intersectionOf (
    time:DateTimeDescription
    [ a owl:Restriction ;
      owl:onProperty time:unitType ;
      owl:hasValue time:unitYear
      ]
    )
  .

:1667 a owl:Class ;
  owl:intersectionOf (
    :YearPrecisionDescription
    [ a owl:Restriction ;
      owl:onProperty time:year ;
      owl:hasValue 1667
      ]
    )
  .

:1668 a owl:Class ;
  owl:intersectionOf (
    :YearPrecisionDescription
    [ a owl:Restriction ;
      owl:onProperty time:year ;
      owl:hasValue 1668
      ]
    )
  .

:1670 a owl:Class ;
  owl:intersectionOf (
    :YearPrecisionDescription
    [ a owl:Restriction ;
      owl:onProperty time:year ;
      owl:hasValue 1670
      ]
    )
  .

:1671 a owl:Class ;
  owl:intersectionOf (
    :YearPrecisionDescription
    [ a owl:Restriction ;
      owl:onProperty time:year ;
      owl:hasValue 1671
      ]
    )
  .

:1672 a owl:Class ;
  owl:intersectionOf (
    :YearPrecisionDescription
    [ a owl:Restriction ;
      owl:onProperty time:year ;
      owl:hasValue 1672
      ]
    )
  .
