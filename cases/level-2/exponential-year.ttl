@base <https://periodo.github.io/edtf-ontology/> .

@prefix : <cases/level-2/exponential-year.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # The calendar year -17*10 to the seventh power = -170000000.
  # 'Y' at the beginning of the string may be followed by an integer,
  # followed by 'E' followed by a positive integer. This signifies
  # "times 10 to the power of". Thus 17E8 means "17 times 10 to the
  # eighth power".
  edtfo:inEDTFDateTime "Y-17E7" ;
  time:inDateTime [
    a time:DateTimeDescription ;
    time:unitType time:unitYear ;
    time:year -170000000
    ]
  .
