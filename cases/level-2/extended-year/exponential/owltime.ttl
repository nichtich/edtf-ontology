PREFIX : <https://periodo.github.io/edtf-ontology/cases/level-2/extended-year/exponential/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX time: <http://www.w3.org/2006/time#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:when1
  a time:Instant ;
  rdfs:label "proleptic Gregorian calendar year 170,000,001 BCE" ;
  time:inDateTime [
      a time:DateTimeDescription ;
      time:unitType time:unitYear ;
      time:year "-170000000"^^xsd:gYear
    ] ;
.

:when2
  a time:Instant ;
  rdfs:label "Gregorian calendar year 17,000" ;
  time:inDateTime [
      a time:DateTimeDescription ;
      time:unitType time:unitYear ;
      time:year "1700"^^xsd:gYear
    ] ;
.

:when3
  a time:Instant ;
  rdfs:label "proleptic Gregorian calendar year 8" ;
  time:inDateTime [
      a time:DateTimeDescription ;
      time:unitType time:unitYear ;
      time:year "0008"^^xsd:gYear
    ] ;
.

