@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-2/individual-qualification-2.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # Month uncertain and approximate; year and day known.
  # A qualification character to the immediate left of a component
  # applies to that component only.
  edtfo:inEDTFDateTime "2004-%06-11" ;
  time:inDateTime [
    a time:DateTimeDescription ;
    time:unitType time:unitDay ;
    time:year 2004 ;
    time:day 11
    ] ;
  time:inDateTime :XXXX_06
  .

:XXXX_06 a time:DateTimeDescription ;
  time:unitType time:unitMonth ;
  time:month 11
  .

:temporalPositionStatement
  a edtfo:UncertainStatement ;
  a edtfo:ApproximateStatement ;
  rdf:subject :when ;
  rdf:predicate time:inDateTime ;
  rdf:object :XXXX_06
  .
