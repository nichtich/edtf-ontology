@base <https://periodo.github.io/edtf-ontology/> .
@prefix : <cases/level-2/interval/qualified-both/> .
@prefix bipm: <http://www.opengis.net/def/trs/BIPM/0/> .
@prefix edtfo: <edtfo.ttl#> .
@prefix iso8601: <http://www.opengis.net/def/uom/ISO-8601/0/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rules: <rules/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:when   a                  time:Interval ;
        rdfs:label         "interval in June 2004 beginning approximately the first and ending possibly but not certainly approximately the 20th" ;
        time:hasBeginning  [ a                time:Instant ;
                             time:inDateTime  _:b0
                           ] ;
        time:hasEnd        [ a                time:Instant ;
                             time:inDateTime  _:b1
                           ] .

<< _:b0 time:day "---01"^^xsd:gDay >>
        a       edtfo:ApproximateStatement .

_:b0    a              time:DateTimeDescription ;
        time:day       "---01"^^xsd:gDay ;
        time:month     "--06"^^xsd:gMonth ;
        time:unitType  time:unitDay ;
        time:year      "2004"^^xsd:gYear .

_:b1    a              time:DateTimeDescription ;
        time:day       "---20"^^xsd:gDay ;
        time:month     "--06"^^xsd:gMonth ;
        time:unitType  time:unitDay ;
        time:year      "2004"^^xsd:gYear .

<< _:b1 time:day "---20"^^xsd:gDay >>
        a       edtfo:ApproximateStatement , edtfo:UncertainStatement .
