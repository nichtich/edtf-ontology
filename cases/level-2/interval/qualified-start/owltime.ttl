PREFIX : <https://periodo.github.io/edtf-ontology/cases/level-2/interval/qualified-start/>
PREFIX edtfo: <https://periodo.github.io/edtf-ontology/edtfo.ttl#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX time: <http://www.w3.org/2006/time#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:when
  a time:Interval ;
  rdfs:label "interval in June 2004 beginning approximately the first and ending the 20th" ;
  time:hasBeginning [
      a time:Instant ;
      time:inDateTime _:n1
    ] ;
  time:hasEnd [
      a time:Instant ;
      time:inDateTime [
          a time:DateTimeDescription ;
          time:day "---20"^^xsd:gDay ;
          time:month "--06"^^xsd:gMonth ;
          time:unitType time:unitDay ;
          time:year "2004"^^xsd:gYear
        ]
    ] ;
.

<urn:triple:1>
  a edtfo:ApproximateStatement ;
  rdf:object "---01"^^xsd:gDay ;
  rdf:predicate time:day ;
  rdf:subject _:n1 ;
.

_:n1
  a time:DateTimeDescription ;
  time:day "---01"^^xsd:gDay ;
  time:month "--06"^^xsd:gMonth ;
  time:unitType time:unitDay ;
  time:year "2004"^^xsd:gYear ;
.

