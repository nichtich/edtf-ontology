@prefix : <https://periodo.github.io/edtf-ontology/cases/level-2/interval/unspecified-both/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
:when a time:Interval .
:when rdfs:label "interval in 2004 beginning on an unspecified day in June ending on the third day of an unspecified month (which must be June or later)" .
:when time:hasBeginning _:b0 .
:when time:hasEnd _:b2 .
_:b0 a time:Instant .
_:b0 time:inDateTime _:b1 .
_:b1 time:month "--06"^^xsd:gMonth .
_:b1 time:unitType time:unitDay .
_:b1 time:year "2004"^^xsd:gYear .
_:b2 a time:Instant .
_:b2 time:inDateTime _:b3 .
_:b3 time:day "---03"^^xsd:gDay .
_:b3 time:unitType time:unitDay .
_:b3 time:year "2004"^^xsd:gYear .
