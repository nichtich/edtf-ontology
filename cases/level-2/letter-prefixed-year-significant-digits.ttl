@base <https://periodo.github.io/edtf-ontology/> .

@prefix : <cases/level-2/letter-prefixed-year-significant-digits.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # Some year between 171000000 and 171999999 estimated to be 171010000.
  # A year may be followed by 'S', followed by a positive integer
  # indicating the number of significant digits.
  edtfo:inEDTFDateTime "Y171010000S3" ;
  time:inDateTime [
    a time:DateTimeDescription ;
    time:unitType [
      a time:TemporalUnit ;
      time:years 1000000
      ] ;
    time:year 171010000
    ]
  .
