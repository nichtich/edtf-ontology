@base <https://periodo.github.io/edtf-ontology/> .
@prefix : <cases/level-2/qualification/group/> .
@prefix edtfo: <edtfo.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Day precision, year and month uncertain
:when1 edtfo:inEDTFDateTime "2004-06?-11" ;
  rdfs:label "possibly but not certainly 2004, possibly but not certainly June, on the 11th day of the month" .

# Day precision, year and month approximate.
:when2 edtfo:inEDTFDateTime "2004-06~-11" ;
  rdfs:label "approximately 2004, approximately June, on the 11th day of the month" .

# Day precision, year and month uncertain and approximate.
:when3 edtfo:inEDTFDateTime "2004-06%-11" ;
  rdfs:label "possibly but not certainly approximately 2004, possibly but not certainly approximately June, on the 11th day of the month" .

# Day precision, year uncertain.
:when4 edtfo:inEDTFDateTime "2004?-06-11" ;
  rdfs:label "possibly but not certainly 2004, on the 11th day of June" .

# Day precision, year approximate.
:when5 edtfo:inEDTFDateTime "2004~-06-11" ;
  rdfs:label "approximately 2004, on the 11th day of June" .

# Day precision, year uncertain and approximate.
:when6 edtfo:inEDTFDateTime "2004%-06-11" ;
  rdfs:label "possibly but not certainly approximately 2004, on the 11th day of June" .

# Month precision, year uncertain.
:when7 edtfo:inEDTFDateTime "2004?-06" ;
  rdfs:label "possibly but not certainly 2004, in June" .

# Month precision, year approximate.
:when8 edtfo:inEDTFDateTime "2004~-06" ;
  rdfs:label "approximately 2004, in June " .

# Month precision, year uncertain and approximate.
:when9 edtfo:inEDTFDateTime "2004%-06" ;
  rdfs:label "possibly but not certainly approximately 2004, in June" .
