@base <https://periodo.github.io/edtf-ontology/> .
@prefix : <cases/level-2/range/on-or-before/> .
@prefix edtfo: <edtfo.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:when1 edtfo:inEDTFDateTime "..1984" ;
  rdfs:label "1984 or some earlier year" .

:when2 edtfo:inEDTFDateTime "..1984-10" ;
  rdfs:label "October 1984 or some earlier month" .

:when3 edtfo:inEDTFDateTime "..1760-02-22" ;
  rdfs:label "February 22, 1760 or some earlier day" .
