@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-2/unspecified-digit-1.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # December 25 sometime during the 1560s.
  edtfo:inEDTFDateTime "156X-12-25" ;
  time:inDateTime  [
    a edtfo:DecadeDescription ;
    time:unitType time:unitDay ;
    edtfo:decade 156 ;
    time:month 12 ;
    time:day 25
    ]
  .
