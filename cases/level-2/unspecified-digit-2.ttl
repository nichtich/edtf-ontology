@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-2/unspecified-digit-2.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # December 25 sometime during the 1500s.
  edtfo:inEDTFDateTime "15XX-12-25" ;
  time:inDateTime  [
    a edtfo:CenturyDescription ;
    time:unitType time:unitDay ;
    edtfo:century 15 ;
    time:month 12 ;
    time:day 25
    ]
  .
