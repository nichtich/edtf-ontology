@base <https://periodo.github.io/edtf-ontology/> .

@prefix : <cases/level-2/unspecified-digit-5.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # Some December during the 1000s.
  edtfo:inEDTFDateTime "1XXX-12" ;
  time:inDateTime  [
    a edtfo:MillenniumDescription ;
    time:unitType time:unitMonth ;
    edtfo:millennium 1 ;
    time:month 12
    ]
  .
