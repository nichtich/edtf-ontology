@base <https://periodo.github.io/edtf-ontology/> .
@prefix : <cases/level-2/unspecified/day-precision/> .
@prefix edtfo: <edtfo.ttl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

:when1 edtfo:inEDTFDateTime "1985-04-XX" ;
  rdfs:label "some day in April 1985" .

:when2 edtfo:inEDTFDateTime "1985-XX-XX" ;
  rdfs:label "some day in 1985" .

:when3 edtfo:inEDTFDateTime "XXXX-XX-XX" ;
  rdfs:label "some unknown day" .
