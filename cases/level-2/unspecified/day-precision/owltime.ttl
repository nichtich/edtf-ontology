PREFIX : <https://periodo.github.io/edtf-ontology/cases/level-2/unspecified/day-precision/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX time: <http://www.w3.org/2006/time#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:when1
  a time:Instant ;
  rdfs:label "some day in April 1985" ;
  time:inDateTime [
      time:month "--04"^^xsd:gMonth ;
      time:unitType time:unitDay ;
      time:year "1985"^^xsd:gYear
    ] ;
.

:when2
  a time:Instant ;
  rdfs:label "some day in some month in 1985" ;
  time:inDateTime [
      time:unitType time:unitDay ;
      time:year "1985"^^xsd:gYear
    ] ;
.

:when3
  a time:Instant ;
  rdfs:label "some day in some month in some year" ;
  time:inDateTime [
      a time:DateTimeDescription ;
      time:unitType time:unitDay
    ] ;
.

