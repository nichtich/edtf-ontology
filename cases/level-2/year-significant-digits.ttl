@base <https://periodo.github.io/edtf-literals/> .

@prefix : <cases/level-2/year-significant-digits.ttl#> .
@prefix edtfo: <edtfo.ttl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix time: <http://www.w3.org/2006/time#> .

<> a owl:Ontology ;
  owl:imports <edtfo.ttl>
  .

:when a time:Instant ;
  # Some year between 1900 and 1999, estimated to be 1950.
  # A year may be followed by 'S', followed by a positive integer
  # indicating the number of significant digits.
  edtfo:inEDTFDateTime "1950S2" ;
  time:inDateTime [
    a time:DateTimeDescription ;
    time:unitType [
      a time:TemporalUnit ;
      time:years 100
      ] ;
    time:year 1950
    ]
  .
