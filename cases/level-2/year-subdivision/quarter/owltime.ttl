PREFIX : <https://periodo.github.io/edtf-ontology/cases/level-2/year-subdivision/quarter/>
PREFIX edtfo: <https://periodo.github.io/edtf-ontology/edtfo.ttl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX time: <http://www.w3.org/2006/time#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:when1
  a time:Instant ;
  rdfs:label "first quarter of 2001" ;
  time:inDateTime [
      a
        time:DateTimeDescription ,
        edtfo:SubYearDateTimeDescription ;
      time:unitType edtfo:unitQuarter ;
      time:year "2001"^^xsd:gYear ;
      edtfo:yearSubdivision edtfo:Quarter1
    ] ;
.

