@base <https://periodo.github.io/edtf-ontology/> .
@prefix : <cases/level-2/year-subdivision/semestral/> .
@prefix bipm: <http://www.opengis.net/def/trs/BIPM/0/> .
@prefix edtfo: <edtfo.ttl#> .
@prefix iso8601: <http://www.opengis.net/def/uom/ISO-8601/0/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:when2  a                time:Instant ;
        time:inDateTime  [ a                      edtfo:SubYearDateTimeDescription , time:DateTimeDescription ;
                           time:hasTRS            iso8601:Gregorian ;
                           time:unitType          edtfo:unitSemestral ;
                           time:year              "2001"^^xsd:gYear ;
                           edtfo:yearSubdivision  edtfo:Semestral2
                         ] .

:when1  a                time:Instant ;
        time:inDateTime  [ a                      edtfo:SubYearDateTimeDescription , time:DateTimeDescription ;
                           time:hasTRS            iso8601:Gregorian ;
                           time:unitType          edtfo:unitSemestral ;
                           time:year              "2001"^^xsd:gYear ;
                           edtfo:yearSubdivision  edtfo:Semestral1
                         ] .
