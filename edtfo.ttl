@base <https://periodo.github.io/edtf-literals/edtfo.ttl> .

@prefix : <#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix edtf: <http://id.loc.gov/datatypes/edtf/> .
@prefix greg: <http://www.w3.org/ns/time/gregorian/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<>
  a owl:Ontology ;
  dct:creator <https://orcid.org/0000-0002-5486-159X> ;
  rdfs:comment "Draft ontology for EDTF concepts."@en ;
  rdfs:label "Extended Date/Time Format (EDTF) concepts"@en ;
  owl:imports <http://www.w3.org/2006/time>
  .

:SubYearDateDescription
  a owl:Class ;
  rdfs:comment """Description of date structured with separate values
for year and a sub-year grouping such as a season or quarter. The
temporal reference system is fixed to Gregorian Calendar and the range
of the year property is restricted to xsd:gYear."""@en ;
  rdf:label "Sub-year date description"@en ;
  rdfs:subClassOf time:DateTimeDescription ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty time:unitType ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:day ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:dayOfWeek ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:dayOfYear ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:hour ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:minute ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:month ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:monthOfYear ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:second ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:timeZone ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "0"^^xsd:nonNegativeInteger ;
      owl:onProperty time:week ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty :yearSubdivision ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty time:year ;
    ] ;
  skos:definition """Description of date structured with separate values
for year and a sub-year grouping such as a season or quarter. The
temporal reference system is fixed to Gregorian Calendar and the range
of the year property is restricted to xsd:gYear."""@en
  .

:YearSubdivision
  a owl:Class ;
  rdfs:comment "A subdivision of the year"@en ;
  rdfs:label "Subdivision of year"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:definition "A subdivision of the year"@en ;
  skos:note """Membership of the class :YearSubdivision is open,
to allow for alternative subdivisions."""@en
  .

:Season
  a owl:Class ;
  rdfs:comment "A season of the year"@en ;
  rdfs:label "Season of year"@en ;
  rdfs:subClassOf :YearSubdivision ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue :unitSeason ;
      owl:onProperty time:unitType ;
    ] ;
  skos:definition "A season of the year"@en ;
  skos:note """Membership of the class :Season is open,
to allow for alternative seasons."""@en
  .

:Quarter
  a owl:Class ;
  rdfs:comment "A quarter of the year"@en ;
  rdfs:label "Quarter of year"@en ;
  rdfs:subClassOf :YearSubdivision ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue :unitQuarter ;
      owl:onProperty time:unitType ;
    ] ;
  owl:oneOf (
    :Quarter1
    :Quarter2
    :Quarter3
    :Quarter4
    ) ;
  skos:definition "A quarter of the year"@en
  .

:Quadrimester
  a owl:Class ;
  rdfs:comment "A quadrimester of the year"@en ;
  rdfs:label "Quadrimester of year"@en ;
  rdfs:subClassOf :YearSubdivision ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue :unitQuadrimester ;
      owl:onProperty time:unitType ;
    ] ;
  owl:oneOf (
    :Quadrimester1
    :Quadrimester2
    :Quadrimester3
    ) ;
  skos:definition "A quadrimester of the year"@en
  .

:Semestral
  a owl:Class ;
  rdfs:comment "A semestral of the year"@en ;
  rdfs:label "Semestral of year"@en ;
  rdfs:subClassOf :YearSubdivision ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:hasValue :unitSemestral ;
      owl:onProperty time:unitType ;
    ] ;
  owl:oneOf (
    :Semestral1
    :Semestral2
    ) ;
  skos:definition "A semestral of the year"@en
  .

:SuperYearDateTimeDescription
  a owl:Class ;
  rdfs:comment """Description of date and time structured with
  separate values for decade or century or millennium, for when the
  date has year precision but a year value is not specified. The
  temporal reference system is fixed to Gregorian Calendar."""@en ;
  rdf:label "Super-year date description"@en ;
  rdfs:subClassOf time:DateTimeDescription ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:cardinality "0"^^xsd:nonNegativeInteger ;
    owl:onProperty time:year ;
    ] ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:hasValue time:unitYear ;
    owl:onProperty time:unitType ;
    ] ;
  skos:definition """Description of date and time structured with
  separate values for decade or century or millennium, for when the
  date has year precision but a year value is not specified. The
  temporal reference system is fixed to Gregorian Calendar."""@en
  .

:DecadeDescription
  a owl:Class ;
  rdf:label "Decade description"@en ;
  rdfs:subClassOf :SuperYearDateTimeDescription ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onProperty :decade
    ]
  .

:CenturyDescription
  a owl:Class ;
  rdf:label "Century description"@en ;
  rdfs:subClassOf :SuperYearDateTimeDescription ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onProperty :century
    ]
  .

:MillenniumDescription
  a owl:Class ;
  rdf:label "Millennium description"@en ;
  rdfs:subClassOf :SuperYearDateTimeDescription ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onProperty :millennium
    ]
  .

[] a owl:AllDisjointClasses ;
  owl:members (
  :DecadeDescription
  :CenturyDescription
  :MillenniumDescription
    )
  .

:OpenEndInterval
  a owl:Class ;
  rdfs:comment """A temporal entity with non-zero extent or duration
  and for which the value of the end is unspecified (perhaps because
  it does not exist but not because it is unknown)"""@en ;
  rdfs:label "Open end interval"@en ;
  rdfs:subClassOf time:ProperInterval ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:cardinality "0"^^xsd:nonNegativeInteger ;
    owl:onProperty time:hasEnd ;
    ] ;
  skos:definition """A temporal entity with non-zero extent or duration
  and for which the value of the end is unspecified (perhaps because
  it does not exist but not because it is unknown)"""@en ;
  .

:OpenBeginningInterval
  a owl:Class ;
  rdfs:comment """A temporal entity with non-zero extent or duration
  and for which the value of the beginning is unspecified (perhaps
  because it does not exist but not because it is unknown)"""@en ;
  rdfs:label "Open beginning interval"@en ;
  rdfs:subClassOf time:ProperInterval ;
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:cardinality "0"^^xsd:nonNegativeInteger ;
    owl:onProperty time:hasBeginning ;
    ] ;
  skos:definition """A temporal entity with non-zero extent or duration
  and for which the value of the beginning is unspecified (perhaps
  because it does not exist but not because it is unknown)"""@en
  .

:uncertainTemporalPosition
  a owl:ObjectProperty ;
  rdfs:comment "Uncertain position of a time instant"@en ;
  rdfs:domain time:Instant ;
  rdfs:label "uncertain temporal position"@en ;
  rdfs:range time:TemporalPosition ;
  rdfs:subPropertyOf time:inTemporalPosition ;
  skos:definition "Uncertain position of a time instant"@en
  .

:approximateTemporalPosition
  a owl:ObjectProperty ;
  rdfs:comment "Approximate position of a time instant"@en ;
  rdfs:domain time:Instant ;
  rdfs:label "approximate temporal position"@en ;
  rdfs:range time:TemporalPosition ;
  rdfs:subPropertyOf time:inTemporalPosition ;
  skos:definition "Approximate position of a time instant"@en
  .

:uncertainApproximateTemporalPosition
  a owl:ObjectProperty ;
  rdfs:comment "Uncertain and approximate position of a time instant"@en ;
  rdfs:domain time:Instant ;
  rdfs:label "uncertain and approximate temporal position"@en ;
  rdfs:range time:TemporalPosition ;
  rdfs:subPropertyOf :uncertainTemporalPosition ;
  rdfs:subPropertyOf :approximateTemporalPosition ;
  skos:definition "Uncertain and approximate position of a time instant"@en
  .

:yearSubdivision
  a owl:ObjectProperty ;
  rdfs:comment """The subdivision of the year,
whose value is a member of the class edtfo:YearSubdivision"""@en ;
  rdfs:domain :SubYearDateDescription ;
  rdfs:label "year subdivision"@en ;
  rdfs:range :YearSubdivision ;
  skos:definition """The subdivision of the year,
whose value is a member of the class edtfo:YearSubdivision"""@en
  .

:hasEDTFDateTimeDescription
  a owl:DatatypeProperty ;
  rdfs:comment "Value of an interval, expressed using EDTF"@en ;
  rdfs:domain time:Interval ;
  rdfs:label "has EDTF datetime description"@en ;
  rdfs:range edtf:EDTF ;
  skos:definition "Value of an interval, expressed using EDTF"@en
  .

:hasEDTFDateTimeSetDescription
  a owl:DatatypeProperty ;
  rdfs:comment """Value of a set of instants, expressed using EDTF

This property is intended for use with EDTF level 2 “all members of set”
(curly brackets) notation. For “one of a set” expressions use
edtfo:inEDTFDateTime.
"""@en ;
  rdfs:domain owl:Class ;
  rdfs:label "has EDTF datetime set description"@en ;
  rdfs:range edtf:EDTF-level2 ;
  skos:definition """Value of a set of instants, expressed using EDTF

This property is intended for use with EDTF level 2 “all members of set”
(curly brackets) notation. For “one of a set” expressions use
edtfo:inEDTFDateTime.
"""@en
  .

:inEDTFDateTime
  a owl:DatatypeProperty ;
  rdfs:comment "Position of an instant, expressed using EDTF"@en ;
  rdfs:domain time:Instant ;
  rdfs:label "in EDTF datetime"@en ;
  rdfs:range edtf:EDTF ;
  skos:definition "Position of an instant, expressed using EDTF"@en
  .

