:decade
  a owl:DatatypeProperty ;
  rdfs:comment """Decade position in the Gregorian calendar;
e.g. 197 is the decade starting with 1970"""@en ;
  rdfs:domain :DecadeDescription ;
  rdfs:label "decade"@en ;
  rdfs:range xsd:integer ;
  skos:definition """Decade position in the Gregorian calendar;
e.g. 197 is the decade starting with 1970"""@en
  .

:century
  a owl:DatatypeProperty ;
  rdfs:comment """Century position in the Gregorian calendar;
e.g. 19 is the century starting with 1900"""@en ;
  rdfs:domain :CenturyDescription ;
  rdfs:label "century"@en ;
  rdfs:range xsd:integer ;
  skos:definition """Century position in the Gregorian calendar;
e.g. 19 is the century starting with 1900"""@en
  .

:millennium
  a owl:DatatypeProperty ;
  rdfs:comment """Millennium position in the Gregorian calendar;
e.g. 1 is the millennium starting with 1000"""@en ;
  rdfs:domain :MillenniumDescription ;
  rdfs:label "millennium"@en ;
  rdfs:range xsd:integer ;
  skos:definition """Millennium position in the Gregorian calendar;
e.g. 1 is the century starting with 1000"""@en
  .

edtf:EDTF
  a owl:Datatype ;
  rdfs:comment """This datatype describes a date and time
that adheres to features of EDTF Levels 0, 1, and 2."""@en ;
  rdfs:label "Extended Date/Time Format"@en ;
  skos:definition """This datatype describes a date and time
that adheres to features of EDTF Levels 0, 1, and 2."""@en
  .

edtf:EDTF-level0
  a owl:Datatype ;
  rdfs:subClassOf edtf:EDTF;
  rdfs:comment """Level 0 features describe dates, dates and times,
and intervals. The latter are represented by start and end
dates. Level 0 supports ISO 8601 features and could be considered a
profile of ISO 8601."""@en ;
  rdfs:label "Extended Date/Time Format - Level 0"@en ;
  owl:onDatatype xsd:string ;
  owl:withRestrictions ([
      xsd:pattern ".*" # TODO
      ]) ;
  skos:definition """Level 0 features describe dates, dates and times,
and intervals. The latter are represented by start and end
dates. Level 0 supports ISO 8601 features and could be considered a
profile of ISO 8601."""@en
  .

edtf:EDTF-level1
  a owl:Datatype ;
  rdfs:subClassOf edtf:EDTF;
  rdfs:comment """Level 1 features describe uncertain, approximate, or
unspecified dates, including provisions for intervals."""@en ;
  rdfs:label "Extended Date/Time Format - Level 1"@en ;
  owl:onDatatype xsd:string ;
  owl:withRestrictions ([
      xsd:pattern ".*" # TODO
      ]) ;
  skos:definition """Level 1 features describe uncertain, approximate, or
unspecified dates, including provisions for intervals."""@en
  .

edtf:EDTF-level2
  a owl:Datatype ;
  rdfs:subClassOf edtf:EDTF;
  rdfs:comment """Level 2 features provide a way to describe dates that
have elements, such as days or months, that are known but which also
have elements that are uncertain, approximate, or unspecified. Level
2 also describes dates that are part of a set."""@en ;
  rdfs:label "Extended Date/Time Format - Level 2"@en ;
  owl:onDatatype xsd:string ;
  owl:withRestrictions ([
      xsd:pattern ".*" # TODO
      ]) ;
  skos:definition """Level 2 features provide a way to describe dates that
have elements, such as days or months, that are known but which also
have elements that are uncertain, approximate, or unspecified. Level
2 also describes dates that are part of a set."""@en
  .

:unitSeason
  a time:TemporalUnit ;
  rdfs:label "Season (unit of temporal duration)"@en ;
  skos:prefLabel "season"@en
  .

:unitQuarter
  a time:TemporalUnit ;
  rdfs:label "Quarter (unit of temporal duration equivalent to 3 months)"@en ;
  skos:prefLabel "quarter"@en ;
  time:days "0"^^xsd:decimal ;
  time:hours "0"^^xsd:decimal ;
  time:minutes "0"^^xsd:decimal ;
  time:months "3"^^xsd:decimal ;
  time:seconds "0"^^xsd:decimal ;
  time:weeks "0"^^xsd:decimal ;
  time:years "0"^^xsd:decimal
  .

:unitQuadrimester
  a time:TemporalUnit ;
  rdfs:label """Quadrimester (unit of temporal duration
equivalent to 4 months)"""@en ;
  skos:prefLabel "quadrimester"@en ;
  time:days "0"^^xsd:decimal ;
  time:hours "0"^^xsd:decimal ;
  time:minutes "0"^^xsd:decimal ;
  time:months "4"^^xsd:decimal ;
  time:seconds "0"^^xsd:decimal ;
  time:weeks "0"^^xsd:decimal ;
  time:years "0"^^xsd:decimal
  .

:unitSemestral
  a time:TemporalUnit ;
  rdfs:label "Semestral (unit of temporal duration equivalent to 6 months)"@en ;
  skos:prefLabel "semestral"@en ;
  time:days "0"^^xsd:decimal ;
  time:hours "0"^^xsd:decimal ;
  time:minutes "0"^^xsd:decimal ;
  time:months "6"^^xsd:decimal ;
  time:seconds "0"^^xsd:decimal ;
  time:weeks "0"^^xsd:decimal ;
  time:years "0"^^xsd:decimal
  .

