PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX re: <https://periodo.github.io/edtf-ontology/rules/regexes/>
PREFIX string: <http://www.w3.org/2000/10/swap/string#>
PREFIX r: <http://www.w3.org/2000/10/swap/reason#>

re:DATE owl:sameAs "-?(?:[1-9][0-9]{3,}|0[0-9]{3})-(?:0[1-9]|1[0-2])-(?:0[1-9]|[12][0-9]|3[01])".
re:TIME owl:sameAs "(?:[01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](?:\\.[0-9]+)?".
re:DATETIME owl:sameAs "-?(?:[1-9][0-9]{3,}|0[0-9]{3})-(?:0[1-9]|1[0-2])-(?:0[1-9]|[12][0-9]|3[01])T(?:[01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](?:\\.[0-9]+)?".
